{
	"synonyms":
	[
		{"click":["press", "tap", "go"]},
		{"write":["enter"]},
		{"edittext":["textbox", "editview"]},
		{"swipe":["slide"]},
		{"greater than":["bigger than"]},
		{"lesser than":["smaller than"]},
		{"equal":["matches", "equals"]},
		{"assert":["verify that", "confirm that", "make sure that", "check if", "see if", "check that"]},
		{"contain":["contains"]},
	],
	"rules":
	[
		{
			"regex":".*(and|with) (the)? text '(.*)'.*",
			"groups":
			[
				{
					"index":"3",
					"type":"selector-text",
					"reliability":"10"
				}
			]
		},
		{
			"regex":".*(and|with) (the)? description '(.*)'.*",
			"groups":
			[
				{
					"index":"3",
					"type":"selector-description",
					"reliability":"10"
				}
			]
		},
		{
			"regex":".*(and|with) (the)? id '(.*)'.*",
			"groups":
			[
				{
					"index":"3",
					"type":"selector-id",
					"reliability":"10"
				}
			]
		},
		{
			"regex":"(click) (on)? (the)? (textview|button|switch|image).*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"4",
					"type":"selector-class",
					"reliability":"10"
				}
			]
		},
		{
			"regex":"(click) (on)? '(.*)'.",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"3",
					"type":"selector-text",
					"reliability":"10"
				}
			]
		},
		{
			"regex":".*on the (switch|button|icon|image).*",
			"groups":
			[
				{
					"index":"1",
					"type":"selector-class",
					"reliability":"10"
				}
			]
		},
		{
			"regex":".*(write) the  text '(.*)'.*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"action-text",
					"reliability":"10"
				}
			]
		},
		{
			"regex":".*(swipe) (to the)? (left|right|top|down).*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"3",
					"type":"action-direction",
					"reliability":"10"
				},
			]
		},
		{
			"regex":"(assert).*'(.*)'( is)? (equal) .*'(.*)'.*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"assert-param-1",
					"reliability":"10"
				},
				{
					"index":"4",
					"type":"assert-type",
					"reliability":"10"
				},
				{
					"index":"5",
					"type":"assert-param-2",
					"reliability":"10"
				},
			]
		},
		{
			"regex":"(assert).*'([0-9]+)'( is)? (greater than|lesser than) .*'([0-9]+)'.*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"assert-param-1",
					"reliability":"10"
				},
				{
					"index":"4",
					"type":"assert-type",
					"reliability":"10"
				},
				{
					"index":"5",
					"type":"assert-param-2",
					"reliability":"10"
				},
			]
		},
		{
			"regex":"(assert).* (switch) is (on|off).*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"selector-class",
					"reliability":"10"
				},
				{
					"index":"3",
					"type":"selector-status",
					"reliability":"10"
				}
			]
		},
		{
			"regex":"(assert).* (edittext) (contain) .* '(.*)'.",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"selector-class",
					"reliability":"10"
				},
				{
					"index":"3",
					"type":"selector-text-type",
					"reliability":"10"
				},
				{
					"index":"4",
					"type":"selector-text",
					"reliability":"10"
				}
			]
		},
		{
			"regex":"(write) the text '(.*)' in the (textbox).*",
			"groups":
			[
				{
					"index":"1",
					"type":"action-type",
					"reliability":"10"
				},
				{
					"index":"2",
					"type":"action-text",
					"reliability":"10"
				},
				{
					"index":"3",
					"type":"selector-class",
					"reliability":"10"
				}
			]
		}
	]
}